{
  "name": "article-feeds-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typeorm": "ts-node --project tsconfig.typeorm.json ./node_modules/typeorm/cli.js",
    "db:generate": "npm run typeorm -- migration:generate src/migrations/InitialMigration -d ormconfig.ts",
    "db:run": "npm run typeorm -- migration:run -d ormconfig.ts",
    "db:revert": "npm run typeorm -- migration:revert -d ormconfig.ts",
    "db:show": "npm run typeorm -- migration:show -d ormconfig.ts",
    "db:drop": "npm run typeorm -- schema:drop -d ormconfig.ts",
    "db:sync": "npm run typeorm -- schema:sync -d ormconfig.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.85.9",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "multer": "^2.0.2",
    "next": "15.5.2",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "reflect-metadata": "^0.2.2",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "typeorm": "^0.3.26",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.12",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
